{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"framework": "nextjs",
	"buildCommand": "cd frontend && npm run build",
	"installCommand": "cd frontend && npm ci",
	"outputDirectory": "frontend/.next",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/frontend/$1"
		}
	],
	"functions": {
		"frontend/app/**": {
			"maxDuration": 30
		}
	}
}